        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ABBConfigMaker"
        mc:Ignorable="d"
        Title="MainWindow" Height="400.5" Width="600">
    <Grid>
        <Button x:Name="btn_loadFile" Content="Load file" HorizontalAlignment="Left" Margin="474,34,0,0" VerticalAlignment="Top" Width="75" Height="23" Click="btn_loadFile_Click"/>
        <TextBox x:Name="txt_loadedFile" HorizontalAlignment="Left" Height="23" Margin="32,34,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="442" IsEnabled="False"/>

    </Grid>
</Window>
